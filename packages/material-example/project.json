{
  "name": "material-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/material-example/src",
  "prefix": "hd",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/material-example",
        "index": "packages/material-example/src/index.html",
        "main": "packages/material-example/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/material-example/tsconfig.app.json",
        "assets": [
          "packages/material-example/src/favicon.ico",
          "packages/material-example/src/assets"
        ],
        "styles": [
          "packages/material-example/src/styles.css",
          "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
          "packages/ui/material/src/style.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "material-example:build:production"
        },
        "development": {
          "buildTarget": "material-example:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "material-example:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/material-example/jest.config.ts"
      }
    }
  },
  "tags": []
}
